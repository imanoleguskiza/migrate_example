    "pdf_text": {
      "bg": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Текстът е актуализиран през ноември 2014 г.</span></p><p class=\"series\">Тази публикация е част от серията „Политиките на Европейския съюз“</p>"
      ],
      "cs": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Rukopis aktualizován v Listopad 2014</span></p><p class=\"series\">Publikace je součástí edice Politiky Evropské unie</p>"
      ],
      "da": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuskript ajourført i november 2014</span></p><p class=\"series\">Denne publikation indgår i serien \"Indblik i EU-politik\"</p>"
      ],
      "de": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuskript aktualisiert im November 2014</span></p><p class=\"series\">Teil der Reihe „Die Europäische Union erklärt“</p>"
      ],
      "el": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Το χειρόγραφο επικαιροποιήθηκε τον Νοέμβριος του 2014.</span></p><p class=\"series\">Η έκδοση αυτή αποτελεί μέρος της σειράς «Η Ευρωπαϊκή Ένωση με απλά λόγια»</p>"
      ],
      "en": [
        "<p class=\"readmore_float\"><span>Manuscript updated in November 2014</span></p><p class=\"series\">This publication is part of the 'European Union explained' series</p>"
      ],
      "es": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Texto original actualizado en noviembre de 2014</span></p><p class=\"series\">Esta publicación forma parte de la serie “Comprender las políticas de la Unión Europea”</p>"
      ],
      "et": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Käsikiri on ajakohastatud november 2014</span></p><p class=\"series\">See väljaanne on osa sarjast „Euroopa Liit lahtiseletatuna”</p>"
      ],
      "fi": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Teksti on päivitetty marraskuu 2014</span></p><p class=\"series\">Julkaisu kuuluu Valokeilassa Euroopan unionin politiikka -sarjaan</p>"
      ],
      "fr": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuscrit actualisé en novembre 2014</span></p><p class=\"series\">Cette publication fait partie de la série «Comprendre les politiques de l’Union européenne»</p>"
      ],
      "ga": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Arna thabhairt cothrom le dáta mí Tachwedd 2014</span></p><p class=\"series\">Cuid den tsraith 'Foras Feasa ar an Aontas Eorpach' is ea an foilseachán seo</p>"
      ],
      "hr": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Rukopis ažuriran u studeni 2014.</span></p><p class=\"series\">Ova publikacija je dio serije \"Objašnjena Europska Unija\"</p>"
      ],
      "hu": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">A kézirat frissítve: 2014. november</span></p><p class=\"series\">A kiadvány a „Közérthetően az Európai Unió szakpolitikáiról” sorozat része.</p>"
      ],
      "it": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Redazione aggiornata ad novembre 2014</span></p><p class=\"series\">Questa pubblicazione fa parte della serie \"Le politiche dell'UE\"</p>"
      ],
      "lt": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Rankraštis atnaujintas 2014 m. novembris mėn.</span></p><p class=\"series\">Tai vienas iš serijos „Apie Europos Sąjungos politiką“ leidinių</p>"
      ],
      "lv": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuskripts atjaunināts 2014. gada novembris.</span></p><p class=\"series\">Šī publikācija izdota sērijā “Ieskats Eiropas Savienības politikā”</p>"
      ],
      "mt": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuskritt aġġornat f'Novembru 2014</span></p><p class=\"series\">Din il-pubblikazzjoni hi parti mis-serje \"Nifhmu l-politiki tal-UE\"</p>"
      ],
      "nl": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Tekst bijgewerkt in november 2014</span></p><p class=\"series\">Deze publicatie maakt deel uit van de reeks \"De Europese Unie in het kort\"</p>"
      ],
      "pl": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Tekst zaktualizowano w listopad 2014 r.</span></p><p class=\"series\">Niniejsza publikacja stanowi część serii „Zrozumieć politykę Unii Europejskiej”.</p>"
      ],
      "pt": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuscrito atualizado em novembro de 2014</span></p><p class=\"series\">Esta brochura faz parte da coleção «Compreender melhor a UE»</p>"
      ],
      "ro": [
        "<p class=\"readmore_float\">Manuscris actualizat în noiembrie 2014</p><p class=\"series\">Această publicaţie face parte din seria „Să înţelegem politicile Uniunii Europene”</p>"
      ],
      "sk": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Rukopis bol aktualizovaný v november 2014</span></p><p class=\"series\">Această publicaţie face parte din seria „Să înţelegem politicile Uniunii Europene”</p>"
      ],
      "sl": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Rokopis je bil posodobljen november 2014.</span></p><p class=\"series\">Publikacija je del zbirke „Politike Evropske unije“.</p>"
      ],
      "sv": [
        "<p class=\"readmore_float\"><span class=\"displayedIn\">Manuskriptet uppdaterat i november 2014</span></p><p class=\"series\">Broschyren ingår i serien ”Insyn i EU-politiken”</p>"
      ]
